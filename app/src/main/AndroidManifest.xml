<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cocktail">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".signupActivity"/>
        <activity android:name=".loginActivity"/>
        <activity android:name=".RecipeActivity"/>
        <activity android:name=".UserInfoActivity"/>
        <activity android:name=".CustomActivity"/>
        <activity android:name=".AddCustomActivity"/>
        <activity android:name=".CustomRecipeActivity"/>
        <activity android:name=".testActivity"/>
        <activity android:name=".FirstQuestionActivity"/>
        <activity android:name=".SecondQuestionActivity"/>
        <activity android:name=".OriginalRecipeActivity"/>
    </application>

</manifest>
